    algorithm
      lambda := 1e-3*Polynomials_Temp.evaluateWithRange(
          {6.5691470817717812E-15,-3.4025961923050509E-11,5.3279284846303157E-08,
          -4.5340839289219472E-05,7.6129675309037664E-02,2.4169481088097051E+01},
          Cv.to_degC(123.15),
          Cv.to_degC(1273.15),
