          "Returns the partial derivative of density with respect to pressure at constant enthalpy"
        algorithm
          ddph := specificHeatCapacityCp(state)/(state.T*data.R*data.R);
          annotation(Inline=true,smoothOrder=2);
        end density_derp_h;

