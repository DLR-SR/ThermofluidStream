          output Temperature T "Temperature";
        protected
          MassFraction[nX] Xfull = if size(X,1) == nX then X else cat(1,X,{1-sum(X)});
        package Internal
            "Solve h(data,T) for T with given h (use only indirectly via temperature_phX)"
