          reference_s=myMedia.Water.StandardWater.specificEntropy(state),
          cp_const=myMedia.Water.StandardWater.specificHeatCapacityCp(state),
          beta_const=myMedia.Water.StandardWater.isobaricExpansionCoefficient(state),
